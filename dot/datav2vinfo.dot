    edge [fontsize=9.0,fontcolor=gray
        ,color=azure
        ,arrowsize=0.6,arrowhead=vee,arrowtail=none
        ,style="setlinewidth(0.5)"
        ];

    src [label="源码",shape=ellipse,style="filled,dashed,setlinewidth(5)"
