        translate->hgtip [label="5%",style="setlinewidth(0.5)"]
        translate->sphnix [label="2%",style="setlinewidth(0.5)"]
        translate->leodoc [label="0%",style="dotted,setlinewidth(0.5)"]
        translate->pyblosxom [label="0%",style="dotted,setlinewidth(0.5)"]
        translate->tpipy [label="0%",style="dotted,setlinewidth(0.5)"]
        translate->pymotw [label="54%",style="dashed,setlinewidth(2)"]
        
    original->lpy [label="20%",style="setlinewidth(2)"]
    original->rwp4l [label="80%",style="setlinewidth(3)"]


    edge [style=invis];
    title->ZQ;
    mailme->OBP;
    
